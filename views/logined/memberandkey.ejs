
<%- include('header') -%>
<%- include('navbar') -%>

<div class="container">
    <h2>Felhasználók és kulcsaik</h2>
    <p>Melyik felhasználónak hova van kulcsa:</p>
    <table class="table table-striped">
        <thead>
        <tr>
            <th>Felhasználó</th>
            <th>Ajtókulcs</th>
            <th></th>
        </tr>
        </thead>
        <tbody>


        <% memberandkey.forEach(function(memberandkey){ %>
        <tr>
            <td><%=memberandkey.member%></td>
            <td><%=memberandkey.door%></td>
            <td><button type="button" class="btn" onclick="location.href='/logined/memberandkey/del?id=<%=memberandkey._id%>';" >Törlés</button></td>
            <% }); %>
        </tr>
        </tbody>
    </table>
    <input type="button" class="btn" value="Új felvétele" onclick="location.href = '/logined/memberandkey/add';">

</div>

<%- include('footer') -%>

