        <thead>
        <tr>
            <th>Név</th>
            <th>Telefonszám</th>
            <th>Email</th>
        </tr>
        </thead>
        <tbody>

        <% member.forEach(function(member){ %>
        <tr>
            <td><%=member.name%></td>
            <td><%=member.phone%></td>
            <td><%=member.email%></td>
        </tr>

        <% }); %>


